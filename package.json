{
  "name": "sign-addon",
  "version": "4.0.1",
  "description": "Signs a Firefox add-on using Mozilla's web service",
  "main": "dist/sign-addon.js",
  "files": [
    "CODE_OF_CONDUCT.md",
    "dist/sign-addon.*",
    "src/**"
  ],
  "scripts": {
    "build": "rimraf dist/ && webpack",
    "changelog": "conventional-changelog -p angular -u",
    "changelog-lint": "commitlint --from master",
    "eslint": "eslint . --ext mjs --ext js",
    "lint": "npm run eslint",
    "prettier": "prettier --write '**'",
    "prettier-ci": "prettier --list-different '**' || (echo '\n\nThis failure means you did not run `npm run prettier-dev` before committing\n\n' && exit 1)",
    "prettier-dev": "pretty-quick --branch master",
    "test": "jest",
    "test-ci": "npm run test -- --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "common-tags": "1.8.2",
    "core-js": "3.22.4",
    "deepcopy": "2.1.0",
    "es6-error": "4.1.1",
    "es6-promisify": "7.0.0",
    "jsonwebtoken": "8.5.1",
    "mz": "2.7.0",
    "request": "2.88.2",
    "source-map-support": "0.5.21",
    "stream-to-promise": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.17.10",
    "@babel/register": "7.17.7",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@types/common-tags": "1.8.1",
    "@types/jest": "27.5.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/mz": "2.7.4",
    "@types/request": "2.48.8",
    "@types/shelljs": "0.8.11",
    "@types/tmp": "0.2.3",
    "babel-jest": "28.0.3",
    "babel-loader": "8.2.5",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.14.0",
    "eslint-config-amo": "5.5.0",
    "eslint-plugin-amo": "1.21.0",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "tmp": "0.2.1",
    "typescript": "4.6.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/sign-addon.git"
  },
  "author": "Kumar McMillan",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/sign-addon/issues"
  },
  "homepage": "https://github.com/mozilla/sign-addon#readme",
  "keywords": [
    "AddOn",
    "Add-on",
    "sign",
    "firefox",
    "WebService"
  ]
}
